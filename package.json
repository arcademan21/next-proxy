{
  "name": "nextjs-proxy",
  "version": "1.1.6",
  "description": "Universal, secure proxy for Next.js. Centralize, audit, and control all external API calls from a single entry point.",
  "author": "Haroldy Arturo Pérez Rodríguez <haroldyarturo@gmail.com>",
  "license": "Public",
  "repository": {
    "type": "git",
    "url": "https://github.com/arcademan21/nextjs-proxy"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --sourcemap",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "test": "jest"
  },
  "keywords": [
    "next",
    "nextjs",
    "nextjs middleware",
    "next proxy",
    "proxy",
    "api",
    "cors",
    "security",
    "logging",
    "rate-limit",
    "transform",
    "gateway"
  ],
  "peerDependencies": {
    "next": ">=13.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/next": "^9.0.0",
    "@types/node": "^24.5.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "tsup": "^8.5.0",
    "typescript": "^5.5.0"
  },
  "sideEffects": false
}
